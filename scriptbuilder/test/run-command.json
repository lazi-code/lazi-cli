{
  "id": "custom-test-run-command",
  "name": "Run Command",
  "category": "Testing",
  "description": "Execute a shell command and capture output",
  "fields": [
    {
      "key": "command",
      "label": "Command",
      "type": "text",
      "placeholder": "ls -la",
      "helperText": "Command to execute",
      "defaultValue": ""
    },
    {
      "key": "outputVariable",
      "label": "Output Variable",
      "type": "text",
      "placeholder": "result",
      "helperText": "Variable to store command output",
      "defaultValue": "output"
    },
    {
      "key": "suppressErrors",
      "label": "Suppress Errors",
      "type": "boolean",
      "helperText": "Continue if command fails",
      "defaultValue": false
    }
  ],
  "generators": {
    "powershell": "(config) => config.suppressErrors ? `$${config.outputVariable} = ${config.command} 2>$null` : `$${config.outputVariable} = ${config.command}`",
    "bash": "(config) => config.suppressErrors ? `${config.outputVariable}=$(${config.command} 2>/dev/null || true)` : `${config.outputVariable}=$(${config.command})`"
  },
  "metadata": {
    "tags": ["processing", "command", "execute"],
    "createdAt": "2025-11-03T00:00:00.000Z",
    "updatedAt": "2025-11-03T00:00:00.000Z"
  }
}
