{
  "id": "custom-test-try-catch",
  "name": "Try-Catch Block",
  "category": "Testing",
  "description": "Wrap commands in error handling",
  "fields": [
    {
      "key": "tryBlock",
      "label": "Try Block",
      "type": "textarea",
      "placeholder": "Commands to try...",
      "helperText": "Commands that might fail",
      "defaultValue": ""
    },
    {
      "key": "catchBlock",
      "label": "Catch Block",
      "type": "textarea",
      "placeholder": "Error handling...",
      "helperText": "What to do if error occurs",
      "defaultValue": "Write-Error \"An error occurred\""
    }
  ],
  "generators": {
    "powershell": "(config) => `try {\\n    ${config.tryBlock}\\n} catch {\\n    ${config.catchBlock}\\n}`",
    "bash": "(config) => `{\\n    ${config.tryBlock}\\n} || {\\n    ${config.catchBlock}\\n}`"
  },
  "metadata": {
    "tags": ["error-handling", "try-catch", "exception"],
    "createdAt": "2025-11-03T00:00:00.000Z",
    "updatedAt": "2025-11-03T00:00:00.000Z"
  }
}
