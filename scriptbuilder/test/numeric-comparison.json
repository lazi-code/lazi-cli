{
  "id": "custom-test-numeric-comparison",
  "name": "Numeric Comparison",
  "category": "Testing",
  "description": "Compare numeric values",
  "fields": [
    {
      "key": "leftValue",
      "label": "Left Value",
      "type": "text",
      "placeholder": "$count",
      "helperText": "Variable or number to compare",
      "defaultValue": ""
    },
    {
      "key": "operator",
      "label": "Operator",
      "type": "select",
      "options": [
        { "label": "Equal (=)", "value": "eq" },
        { "label": "Not Equal (≠)", "value": "ne" },
        { "label": "Greater Than (>)", "value": "gt" },
        { "label": "Greater or Equal (≥)", "value": "ge" },
        { "label": "Less Than (<)", "value": "lt" },
        { "label": "Less or Equal (≤)", "value": "le" }
      ],
      "defaultValue": "gt"
    },
    {
      "key": "rightValue",
      "label": "Right Value",
      "type": "text",
      "placeholder": "10",
      "helperText": "Value to compare against",
      "defaultValue": "0"
    },
    {
      "key": "errorMessage",
      "label": "Error Message",
      "type": "text",
      "placeholder": "Comparison failed",
      "helperText": "Message if comparison is false",
      "defaultValue": "Comparison failed"
    }
  ],
  "generators": {
    "powershell": "(config) => { const ops = { eq: '-eq', ne: '-ne', gt: '-gt', ge: '-ge', lt: '-lt', le: '-le' }; return `if (-not (${config.leftValue} ${ops[config.operator]} ${config.rightValue})) {\\n    Write-Error \"${config.errorMessage}\"\\n    exit 1\\n}`; }",
    "bash": "(config) => { const ops = { eq: '-eq', ne: '-ne', gt: '-gt', ge: '-ge', lt: '-lt', le: '-le' }; return `if [ ! ${config.leftValue} ${ops[config.operator]} ${config.rightValue} ]; then\\n    echo \"${config.errorMessage}\" >&2\\n    exit 1\\nfi`; }"
  },
  "metadata": {
    "tags": ["validation", "numeric", "comparison"],
    "createdAt": "2025-11-03T00:00:00.000Z",
    "updatedAt": "2025-11-03T00:00:00.000Z"
  }
}
