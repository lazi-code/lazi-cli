{
  "metadata": {
    "name": "Batch File Processing",
    "description": "Lists files in a directory and processes each one in a loop",
    "author": "Test Suite",
    "createdAt": "2025-01-04T00:00:00.000Z",
    "scriptType": "bash",
    "version": "1.0"
  },
  "workflow": {
    "nodes": [
      {
        "id": "node-1",
        "type": "scriptNode",
        "position": {
          "x": 100,
          "y": 200
        },
        "data": {
          "script": {
            "id": "custom-test-list-files",
            "name": "List Files",
            "category": "Input"
          },
          "config": {
            "directory": "/home/user/documents",
            "pattern": "*.txt",
            "recursive": false
          }
        }
      },
      {
        "id": "node-2",
        "type": "scriptNode",
        "position": {
          "x": 400,
          "y": 200
        },
        "data": {
          "script": {
            "id": "custom-test-for-loop-range",
            "name": "For Loop (Range)",
            "category": "Control Flow"
          },
          "config": {
            "iteratorVar": "i",
            "startValue": 0,
            "endValue": 10,
            "step": 1
          }
        }
      },
      {
        "id": "node-3",
        "type": "scriptNode",
        "position": {
          "x": 700,
          "y": 150
        },
        "data": {
          "script": {
            "id": "custom-test-run-command",
            "name": "Run Command",
            "category": "Processing"
          },
          "config": {
            "command": "wc -l ${files[$i]}",
            "outputVariable": "lineCount",
            "suppressErrors": false
          }
        }
      },
      {
        "id": "node-4",
        "type": "scriptNode",
        "position": {
          "x": 1000,
          "y": 150
        },
        "data": {
          "script": {
            "id": "custom-test-print-output",
            "name": "Print Output",
            "category": "Output"
          },
          "config": {
            "message": "\"File ${files[$i]}: $lineCount lines\"",
            "outputType": "info"
          }
        }
      },
      {
        "id": "node-5",
        "type": "scriptNode",
        "position": {
          "x": 700,
          "y": 300
        },
        "data": {
          "script": {
            "id": "custom-test-log-message",
            "name": "Log Message",
            "category": "Error Handling"
          },
          "config": {
            "logFile": "/tmp/batch-processing.log",
            "message": "\"Batch processing completed\"",
            "includeTimestamp": true
          }
        }
      }
    ],
    "edges": [
      {
        "id": "edge-1",
        "source": "node-1",
        "target": "node-2",
        "type": "smoothstep"
      },
      {
        "id": "edge-2",
        "source": "node-2",
        "sourceHandle": "loop-body",
        "target": "node-3",
        "type": "smoothstep"
      },
      {
        "id": "edge-3",
        "source": "node-3",
        "target": "node-4",
        "type": "smoothstep"
      },
      {
        "id": "edge-4",
        "source": "node-2",
        "sourceHandle": "after-loop",
        "target": "node-5",
        "type": "smoothstep"
      }
    ]
  },
  "requiredCustomNodes": [
    {
      "id": "custom-test-list-files",
      "name": "List Files",
      "category": "Input",
      "description": "Get a list of files from a directory",
      "fields": [
        {
          "key": "directory",
          "label": "Directory Path",
          "type": "text",
          "placeholder": "/path/to/directory",
          "helperText": "Directory to list files from",
          "defaultValue": "."
        },
        {
          "key": "pattern",
          "label": "File Pattern",
          "type": "text",
          "placeholder": "*.txt",
          "helperText": "Pattern to filter files (e.g., *.log)",
          "defaultValue": "*"
        },
        {
          "key": "recursive",
          "label": "Recursive",
          "type": "boolean",
          "helperText": "Include subdirectories",
          "defaultValue": false
        }
      ],
      "generators": {
        "powershell": "(config) => config.recursive ? `$files = Get-ChildItem -Path \"${config.directory}\" -Filter \"${config.pattern}\" -Recurse` : `$files = Get-ChildItem -Path \"${config.directory}\" -Filter \"${config.pattern}\"`",
        "bash": "(config) => config.recursive ? `files=$(find \"${config.directory}\" -name \"${config.pattern}\")` : `files=$(ls \"${config.directory}\"/${config.pattern})`"
      },
      "metadata": {
        "tags": ["input", "files", "directory"],
        "createdAt": "2025-01-04T00:00:00.000Z",
        "updatedAt": "2025-01-04T00:00:00.000Z"
      }
    },
    {
      "id": "custom-test-for-loop-range",
      "name": "For Loop (Range)",
      "category": "Control Flow",
      "description": "Iterate over a numeric range",
      "outputHandles": [
        {
          "id": "loop-body",
          "label": "Loop Body",
          "position": "bottom",
          "color": "#2563eb"
        },
        {
          "id": "after-loop",
          "label": "After Loop",
          "position": "right",
          "color": "#64748b"
        }
      ],
      "fields": [
        {
          "key": "iteratorVar",
          "label": "Iterator Variable",
          "type": "text",
          "placeholder": "i",
          "helperText": "Variable name for loop counter",
          "defaultValue": "i"
        },
        {
          "key": "startValue",
          "label": "Start Value",
          "type": "number",
          "placeholder": "0",
          "helperText": "Starting number (inclusive)",
          "defaultValue": 0
        },
        {
          "key": "endValue",
          "label": "End Value",
          "type": "number",
          "placeholder": "10",
          "helperText": "Ending number (exclusive for PowerShell, inclusive for Bash)",
          "defaultValue": 10
        },
        {
          "key": "step",
          "label": "Step",
          "type": "number",
          "placeholder": "1",
          "helperText": "Increment amount",
          "defaultValue": 1
        }
      ],
      "generators": {
        "powershell": "(config, branches) => { const iter = config.iteratorVar || 'i'; const start = config.startValue ?? 0; const end = config.endValue ?? 10; const step = config.step ?? 1; let code = `for ($${iter} = ${start}; $${iter} -lt ${end}; $${iter} += ${step}) {\\n`; if (branches?.['loop-body']) { code += `    ${branches['loop-body'].replace(/\\n/g, '\\n    ')}\\n`; } code += `}`; return code; }",
        "bash": "(config, branches) => { const iter = config.iteratorVar || 'i'; const start = config.startValue ?? 0; const end = config.endValue ?? 10; const step = config.step ?? 1; let code = `for ${iter} in $(seq ${start} ${step} ${end}); do\\n`; if (branches?.['loop-body']) { code += `    ${branches['loop-body'].replace(/\\n/g, '\\n    ')}\\n`; } code += `done`; return code; }"
      },
      "metadata": {
        "tags": ["control-flow", "loop", "for", "range"],
        "createdAt": "2025-01-04T00:00:00.000Z",
        "updatedAt": "2025-01-04T00:00:00.000Z"
      }
    },
    {
      "id": "custom-test-run-command",
      "name": "Run Command",
      "category": "Processing",
      "description": "Execute a shell command and capture output",
      "fields": [
        {
          "key": "command",
          "label": "Command",
          "type": "text",
          "placeholder": "ls -la",
          "helperText": "Command to execute",
          "defaultValue": ""
        },
        {
          "key": "outputVariable",
          "label": "Output Variable",
          "type": "text",
          "placeholder": "result",
          "helperText": "Variable to store command output",
          "defaultValue": "output"
        },
        {
          "key": "suppressErrors",
          "label": "Suppress Errors",
          "type": "boolean",
          "helperText": "Continue if command fails",
          "defaultValue": false
        }
      ],
      "generators": {
        "powershell": "(config) => config.suppressErrors ? `$${config.outputVariable} = ${config.command} 2>$null` : `$${config.outputVariable} = ${config.command}`",
        "bash": "(config) => config.suppressErrors ? `${config.outputVariable}=$(${config.command} 2>/dev/null || true)` : `${config.outputVariable}=$(${config.command})`"
      },
      "metadata": {
        "tags": ["processing", "command", "execute"],
        "createdAt": "2025-01-04T00:00:00.000Z",
        "updatedAt": "2025-01-04T00:00:00.000Z"
      }
    },
    {
      "id": "custom-test-print-output",
      "name": "Print Output",
      "category": "Output",
      "description": "Display text or variable to console",
      "fields": [
        {
          "key": "message",
          "label": "Message",
          "type": "textarea",
          "placeholder": "Hello, World!",
          "helperText": "Text or variable to display",
          "defaultValue": ""
        },
        {
          "key": "outputType",
          "label": "Output Type",
          "type": "select",
          "options": [
            { "label": "Information", "value": "info" },
            { "label": "Warning", "value": "warning" },
            { "label": "Error", "value": "error" }
          ],
          "defaultValue": "info"
        }
      ],
      "generators": {
        "powershell": "(config) => { if (config.outputType === 'warning') return `Write-Warning ${config.message}`; if (config.outputType === 'error') return `Write-Error ${config.message}`; return `Write-Host ${config.message}`; }",
        "bash": "(config) => { if (config.outputType === 'error') return `echo ${config.message} >&2`; return `echo ${config.message}`; }"
      },
      "metadata": {
        "tags": ["output", "print", "console"],
        "createdAt": "2025-01-04T00:00:00.000Z",
        "updatedAt": "2025-01-04T00:00:00.000Z"
      }
    },
    {
      "id": "custom-test-log-message",
      "name": "Log Message",
      "category": "Error Handling",
      "description": "Write a message to a log file",
      "fields": [
        {
          "key": "logFile",
          "label": "Log File",
          "type": "text",
          "placeholder": "/var/log/script.log",
          "helperText": "Path to log file",
          "defaultValue": "script.log"
        },
        {
          "key": "message",
          "label": "Message",
          "type": "textarea",
          "placeholder": "Log entry...",
          "helperText": "Message to log",
          "defaultValue": ""
        },
        {
          "key": "includeTimestamp",
          "label": "Include Timestamp",
          "type": "boolean",
          "defaultValue": true
        }
      ],
      "generators": {
        "powershell": "(config) => config.includeTimestamp ? `Add-Content -Path \"${config.logFile}\" -Value \"$(Get-Date -Format 'yyyy-MM-dd HH:mm:ss') - ${config.message}\"` : `Add-Content -Path \"${config.logFile}\" -Value ${config.message}`",
        "bash": "(config) => config.includeTimestamp ? `echo \"$(date '+%Y-%m-%d %H:%M:%S') - ${config.message}\" >> \"${config.logFile}\"` : `echo ${config.message} >> \"${config.logFile}\"`"
      },
      "metadata": {
        "tags": ["error-handling", "logging", "file"],
        "createdAt": "2025-01-04T00:00:00.000Z",
        "updatedAt": "2025-01-04T00:00:00.000Z"
      }
    }
  ]
}
